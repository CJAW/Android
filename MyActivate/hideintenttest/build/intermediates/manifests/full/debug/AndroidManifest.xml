<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.hideintenttest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.user.hideintenttest.TestHideIntent"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        //绑定界面
        <activity android:name="com.example.user.hideintenttest.Users" >
            <intent-filter>
  //过滤
                <action android:name="com.example.user.hidintentTest.Users" />

                <category android:name="android.intent.category.DEFAULT" />
// 可以使用任意的字符串，通过字符串找到界面，但要是用格式
            </intent-filter>
        </activity>
    </application>

</manifest>