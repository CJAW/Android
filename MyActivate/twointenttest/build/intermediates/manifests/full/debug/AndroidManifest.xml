<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.twointenttest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.user.twointenttest.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.user.twointenttest.myaty1"
            android:label="@string/title_activity_myaty1"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                //过滤
                <action android:name="com.example.user.twointenttest.myaty" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.user.twointenttest.myaty2"
            android:label="@string/title_activity_myaty2"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="com.example.user.twointenttest.myaty" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="app" />
 //自定义协议，可快速定位界面
            </intent-filter>
        </activity>
    </application>

</manifest>